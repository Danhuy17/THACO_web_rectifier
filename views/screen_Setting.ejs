    <meta name="author" content="Duy Anh - R&D">
    <title> <%= __('RECTIFIER MONITORING SYSTEM') %> </title>

    <script src="socket.io/socket.io.js"></script>
    

    <!--______________KHỞI TẠO SOCKET.IO______________ -->
    <script>
        var socket = io("localhost:3000")
        socket.on('error', function (message) {
            alert(message);
        });
    </script>

    <!--______________KHAI BÁO CÁC SOURCE______________ -->
    <script src="https://kit.fontawesome.com/212982cf09.js" crossorigin="anonymous"></script>
    <script src="js/plotly-2.8.3.min.js"></script>
    <script src="js/jquery-3.7.0.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-Knob/1.2.13/jquery.knob.min.js"></script>
    <script src="js/FileSaver.js"></script>

    <!--______________ĐƯỜNG DẪN CÁC FILE FUNCTION JS______________ -->
    <script src="js/FC1_Common.js"></script>



    <!--______________ĐƯỜNG DẪN CSS______________ -->
    <link rel="stylesheet" href="CSS/root.css">
    <link rel="stylesheet" href="CSS/header.css">
    <link rel="stylesheet" href="CSS/screen_Setting.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">


    <!--______________ĐƯỜNG DẪN FAVICON______________ -->
    <link rel="icon" href="images/favicon.ico" type="image/x-icon">

</head>

<body>
    <!-- Body -->
    <div id="Screen_Setting">
    
        <!-- Danh sách các cài đặt -->
        <div class="list-container">
    
            <!-- điện áp hoạt động CK 1 -->
            <div class="list-item">
                <div class="list-knob">
                    <input type="text" value="75" class="knob-Work_Voltage_1" data-value="75">
                    <script>
                        $(".knob-Work_Voltage_1").knob({
                            'width': 180,
                            'height': 180,
                            'fgColor': "#FF0000",
                            'inputColor': "FF0000",
                            'min': 0,
                            'max': 2000,
                            'angleArc': 250,
                            'angleOffset': -125
                        });
                    </script>
                </div>
                <p class="list-text">
                    <%= __('THE FIRST OPERATING VOLTAGE') %>
                </p>
            </div>
    
            <!-- điện áp hoạt động CK 2 -->
            <div class="list-item">
