<div id="Screen_Data">

    <div class="data-upper">

        <div class="data-upper_date">

            <div class="data-upper_from">
                <label class="label_search_start_date">
                    <p style="color: white;">
                        <%= __('FROM') %>
                    </p>
                </label>
                <input id="input_search_start_date" type="datetime-local">
            </div>

            <div class="data-upper_to">
                <label class="label_search_end_date">
                    <p style="color: white;">
                        <%= __('TO') %>
                    </p>
                </label>
                <input id="input_search_end_date" type="datetime-local">
            </div>

        </div>


        <div class="data-upper_button">
            <!-- Search button -->
            <button class="data-upper_button__style" onclick="fn_Data_By_Time();">
                <%= __('SEARCH') %>
            </button>

            <!-- Refresh button -->
            <button class="data-upper_button__style" onclick="fn_Exit_Search_Data();">
                <%= __('REFRESH') %>
            </button>

            <!-- Export to Excel button -->
            <button class="data-upper_button__style" onclick="fn_excel_data();">
                <%= __('EXPORT EXCEL FILE') %>
            </button>
        </div>

    </div>

    <div class="data-lower">
        <table id="table_Data">

            <!-- Tiêu đề bảng -->
            <thead align="center">

                <tr> <!-- Tiêu đề các cột -->
                    <th>
                        <%= __('DATE') %>
                    </th>

                    <th>
                        <%= __('DC CURRENT') %>
                    </th>

                    <th>
                        <%= __('DC VOLTAGE') %>
                    </th>
                </tr>

            </thead>

        </table>

    </div>


</div>
