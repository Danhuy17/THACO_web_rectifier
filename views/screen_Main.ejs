<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="Duy Anh - R&D">
    <title>
        <%= __('RECTIFIER MONITORING SYSTEM') %>
    </title>

    <script src="socket.io/socket.io.js"></script>


    <!--______________KHỞI TẠO SOCKET.IO______________ -->
    <script>
        var socket = io("localhost:3000")
        socket.on('error', function (message) {
            alert(message);
        });
    </script>

    <!--______________TRUYỀN TÊN VIET/ENG______________ -->
    <script>
        var noErroredModuleText = '<%= __("NO ERRORED MODULE") %>';
    </script>

    <!--______________KHAI BÁO CÁC SOURCE______________ -->
    <script src="https://kit.fontawesome.com/212982cf09.js" crossorigin="anonymous"></script>
    <script src="js/plotly-2.8.3.min.js"></script>
    <script src="js/jquery-3.7.0.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-Knob/1.2.13/jquery.knob.min.js"></script>

    <!--______________ĐƯỜNG DẪN CÁC FILE FUNCTION JS______________ -->
    <script src="js/FC1_Common.js"></script>


    <!--______________ĐƯỜNG DẪN CSS______________ -->
    <link rel="stylesheet" href="CSS/root.css">
    <link rel="stylesheet" href="CSS/header.css">
    <link rel="stylesheet" href="CSS/screen_main.css">
    <link rel="stylesheet" href="CSS/preload-effect.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">


    <!--______________ĐƯỜNG DẪN FAVICON______________ -->
    <link rel="icon" href="images/favicon.ico" type="image/x-icon">

</head>

<body>

    <div id="Screen_Main">
        <script> fn_ImageChange("EDA", "ED", "ED_working") </script>
        <%- include ('preload-effect.ejs') %>


            <!-- Main -->
            <div class="list-container">

                <!-- DC Current -->
                <div class="list-item">
                    <div class="list-knob">
                        <input type="text" value="75" class="knob-Current-DC" data-value="75">
                        <script>
                            $(".knob-Current-DC").knob({
                                'width': 180,
                                'height': 180,
                                'fgColor': "#FF0000",
                                'inputColor': "FF0000",
                                'min': 0,
                                'max': 2000,
                                'angleArc': 250,
                                'angleOffset': -125
                            });
                        </script>
                    </div>
                    <p class="list-text">
                        <%= __('DC CURRENT') %>
                    </p>
                </div>

                <!-- DC Voltage -->
                <div class="list-item">
                    <div class="list-knob">
                        <input type="text" value="75" class="knob-Voltage-DC" data-value="75">
                        <script>
                            $(".knob-Voltage-DC").knob({
                                'width': 180,
                                'height': 180,
                                'fgColor': "#FF0000",
                                'inputColor': "FF0000",
                                'min': 0,
                                'max': 1000,
                                'angleArc': 250,
                                'angleOffset': -125
                            });
                        </script>
                    </div>
                    <p class="list-text">
                        <%= __('DC VOLTAGE') %>
                    </p>
                </div>

                <!-- Given Voltage -->
                <div class="list-item">
                    <div class="list-knob">
                        <input type="text" value="75" class="knob-Given-voltage" data-value="75">
                        <script>
                            $(".knob-Given-voltage").knob({
                                'width': 180,
                                'height': 180,
                                'fgColor': "rgb(147,42,182)",
                                'inputColor': "rgb(147,42,182)",
                                'min': 0,
                                'max': 1000,
                                'angleArc': 250,
                                'angleOffset': -125
                            });
                        </script>
                    </div>
                    <p class="list-text">
                        <%= __('GIVEN VOLTAGE') %>
                    </p>
                </div>

                <!-- Time left I -->
                <div class="list-item">
                    <div class="list-knob">
                        <input type="text" value="75" class="knob-Time-1" data-value="75">
                        <script>
                            $(".knob-Time-1").knob({
                                'width': 180,
                                'height': 180,
                                'fgColor': "rgb(60,141,188)",
                                'inputColor': "rgb(60,141,188)",
                                'min': 0,
                                'max': 200,
                                'angleArc': 250,
                                'angleOffset': -125
                            });
                        </script>
                    </div>
                    <p class="list-text">
                        <%= __('REMAINING TIME OF PROCESS I') %>
                    </p>
                </div>

                <!-- Time left II -->
                <div class="list-item">
                    <div class="list-knob">
                        <input type="text" value="75" class="knob-Time-2" data-value="75">
                        <script>
                            $(".knob-Time-2").knob({
                                'width': 180,
                                'height': 180,
                                'fgColor': "rgb(60,141,188)",
                                'inputColor': "rgb(60,141,188)",
                                'min': 0,
                                'max': 200,
                                'angleArc': 250,
                                'angleOffset': -125
                            });
                        </script>
                    </div>
                    <p class="list-text">
                        <%= __('REMAINING TIME OF PROCESS II') %>
                    </p>
                </div>

                <!-- Number of working modules -->
                <div class="list-item">
                    <div class="list-knob">
                        <input type="text" value="75" class="knob-num-modules" data-value="75">
                        <script>
                            $(".knob-num-modules").knob({
                                'width': 180,
                                'height': 180,
                                'fgColor': "rgb(0,166,90)",
                                'inputColor': "rgb(0,166,90)",
                                'min': 0,
                                'max': 42,
                                'angleArc': 250,
                                'angleOffset': -125
                            });
                        </script>
                    </div>
                    <p class="list-text">
                        <%= __('NUMBER OF WORKING MODULES') %>
                    </p>
                </div>

            </div>

            <img id="EDA">

            <div class="shape_fault">
                <%= __('ERRORED MODULES') %>
                    <div class="inner_shape_fault"></div>
            </div>
    </div>

    <!-- Header -->
    <%- include ('header.ejs') %>

    <!--______________KHAI BÁO CÁC FILE SOCKET______________ -->
    <script src="/js/socket/main.js"></script>

</body>